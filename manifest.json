{
    "manifest_version": 3,
    "name": "LeetCode Question Finder",
    "version": "1.0",
    "description": "Opens a LeetCode question by its question number.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "finder_icon.png",
        "48": "finder_icon.png",
        "128": "finder_icon.png"
      }
    },
    "permissions": ["tabs"],
    "web_accessible_resources": [
      {
        "resources": ["questions.json"],
        "matches": ["<all_urls>"]
      }
    ]
  }